{{!-- Header Page --}}
<header class="Header white z-depth-1 navbar-fixed">
  <div class="u-wrap">
    <div class="Header-wrap">
      <div class="col s9">
        {{!-- Add Logo --}}
        <a class="Logo Inline col s4" href="{{@blog.url}}">
          {{#if @blog.logo}}
          <img  class="brand-logoImg" src="{{@blog.logo}}" alt="{{@blog.title}}"/>{{else}} <span>{{@blog.title}}</span>
          {{/if}}
        </a>
        {{!-- Menu --}}
        {{!-- <a class="right" href="#Menu">Menu</a> --}}
        {{!-- Form --}}
        <div class="Header-form Inline col s8">
          <form>
              <input id="Search" placeholder="Buscar" type="text"/>
          </form>
        </div>
      </div>
    </div>
  </div>
</header>
